**FREE
/********************************************************************/
/* Program : MGRCANDTL                                              */
/* Purpose : Manager - View Cancellation Details                   */
/********************************************************************/
ctl-opt dftactgrp(*no)
        actgrp(*caller);

/*------------------------------------------------------------------*/
/* Files                                                            */
/*------------------------------------------------------------------*/
dcl-f MGRCANDDSP workstn;
dcl-f CANCLFMGR usage(*input) keyed;

/*------------------------------------------------------------------*/
/* Parameter                                                        */
/*------------------------------------------------------------------*/
dcl-pi *n;
   pCanId packed(7:0);
end-pi;

/*------------------------------------------------------------------*/
/* Fetch Cancellation                                               */
/*------------------------------------------------------------------*/
chain pCanId CANCLFMGR;
if %notfound(CANCLFMGR);
   return;
endif;

/*------------------------------------------------------------------*/
/* Load Display                                                     */
/*------------------------------------------------------------------*/
DSP_CANID  = CANCID;
DSP_EMP    = EMPID;
DSP_CAB    = CABID;
DSP_DATE   = CANCDATE;
DSP_REASN  = REASON;
DSP_STAT   = STATUS;

/*------------------------------------------------------------------*/
/* Show Window                                                      */
/*------------------------------------------------------------------*/
exfmt CANDTLW;

return;
